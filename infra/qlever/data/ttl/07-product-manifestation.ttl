  rdfs:domain narrative:Product ;
  rdfs:range xsd:boolean
.

#################################################################
# Video game / digital interactive product refinements
#################################################################

narrative:editionType
  a owl:DatatypeProperty ;
  rdfs:label "edition type" ;
